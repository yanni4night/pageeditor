{
    "name": "手游SEM(全页链接)",
    "date": "2014-10-28",
    "desc": "支持单下载链接与多下载链接，如果是多下载链接，请将URL中要使用apk参数的部分用{apkid}代替，如http://dl.com/cdn/prefix_{apkid}.apk",
    "url": "http://wan.sogou.com/static/sds_wap/{{name}}.html",
    "ctrljs": "sds_wap.js",
    "checkUrl": true,
    "fields": [{
        "name": "gameName",
        "string": "string",
        "desc": "游戏名称(中文)，如3D坦克"
    }, {
        "name": "pageName",
        "string": "word",
        "desc": "页面名称(英文)，如mkift"
    }, {
        "name": "bg",
        "string": "url",
        "desc": "背景图(最低320px宽度，最佳640px,jpg最佳)，http://..."
    }, {
        "name": "downloadUrl",
        "string": "url",
        "desc": "下载链接，http://..."
    }, {
        "name": "bgColor",
        "string": true,
        "desc": "背景色(与图相近)，如#1FF09D"
    }]
}