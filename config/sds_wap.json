{
    "name": "手游SEM",
    "date": "2014-09-15",
    "desc":"支持单下载链接与多下载链接，如果是多下载链接，请将URL中要使用apk参数的部分用{apkid}代替，如http://dl.com/cdn/prefix_{apkid}.apk",
    "url": "http://wan.sogou.com/static/sds_wap/{{name}}.html",
    "ctrljs":"sds_wap.js",
    "checkUrl": true,
    "fields": [{
        "name": "gameName",
        "string": "string",
        "desc": "游戏名称(中文)，如3D坦克"
    }, {
        "name": "pageName",
        "string": "word",
        "desc": "页面名称(英文)，如mkift"
    }, {
        "name": "topBg",
        "string": "url",
        "desc": "顶部图(640*736,jpg最佳)，http://..."
    }, {
        "name": "bottomBg",
        "string": "url",
        "desc": "底部图(640*1266,jpg最佳)，http://..."
    }, {
        "name": "downloadUrl",
        "string": "url",
        "desc": "下载链接，http://..."
    }, {
        "name": "hasBottom",
        "boolean": true,
        "desc": "是否需要底部"
    }, {
        "name": "bgColor",
        "string": true,
        "desc": "背景色(与图相近)，如#1FF09D"
    }]
}